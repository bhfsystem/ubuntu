{
  "version": "2.1",
  "networks": {
    "default": {
      "external": {
        "name": "ubuntu_default"
      }
    }
  },
  "services": {
    "ubuntu": {
      "volumes": [
        "/data:/data",
        "/config:/config",
        "/var/run/docker.sock:/var/run/docker.sock"
      ],
      "dns_search": "node.dc1.consul service.dc1.consul",
      "image": "${BLOCK_REGISTRY}/block:ubuntu"
    }
  }
}
