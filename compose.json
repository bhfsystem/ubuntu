{
  "version": "2.1",
  "networks": {
    "default": {
      "ipam": {
        "config": [
          {
            "subnet": "${NET_SUBNET}.0/23",
            "gateway": "${NET_SUBNET}.1",
            "ip_range": "${NET_PREFIX}.128/25"
          }
        ]
      }
    }
  },
  "services": {
    "base": {
      "environment": [
      ],
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns_search": "node.dc1.consul service.dc1.consul",
      "image": "${BLOCK_REGISTRY}/block:base"
    },
    "ubuntu": {
      "environment": [
      ],
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns_search": "node.dc1.consul service.dc1.consul",
      "image": "${BLOCK_REGISTRY}/block:ubuntu"
    }
  }
}
