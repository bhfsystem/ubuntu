#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source normalize  

  cd "$shome"
  git grep cache.nih | grep template: | grep -v exec/ | cut -d: -f1 | uniq | runmany 'cp $1 ${1%.template}'
}

source sub "$BASH_SOURCE" "$@"
